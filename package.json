{
  "name": "cousin-cli",
  "version": "2.0.0",
  "description": "A Front-End Engineering Scaffold For Lint and Building",
  "bin": {
    "cousin": "bin/index.js"
  },
  "scripts": {
    "lint": "eslint . --fix --ext .js,.json",
    "prettier": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abcrun/cousin.git"
  },
  "author": "hrdesign@163.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abcrun/cousin/issues"
  },
  "keywords": [
    "Front End Engineering Scaffold",
    "Lint",
    "Build",
    "Webpack",
    "React",
    "Typescript",
    "Less",
    "Sass",
    "Postcss",
    "CSS Modules",
    "前端工程化脚手架",
    "构建"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^9.0.1",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.3.2"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commander": "^8.0.0",
    "execa": "^5.1.1",
    "fs-extra": "^9.0.1",
    "hard-source-webpack-plugin": "latest",
    "inquirer": "^7.2.0",
    "leven": "^3.1.0",
    "mem-fs": "^1.2.0",
    "mem-fs-editor": "^7.0.1",
    "mini-css-extract-plugin": "latest",
    "mocker-api": "^2.3.5",
    "optimize-css-assets-webpack-plugin": "latest",
    "postcss": "^8.0.0",
    "postcss-import": "latest",
    "postcss-preset-env": "latest",
    "semver": "^7.3.2",
    "uglifyjs-webpack-plugin": "latest",
    "validate-npm-package-name": "^3.0.0",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.14",
    "webpack": "latest",
    "webpack-bundle-analyzer": "latest",
    "webpack-dev-server": "latest"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
