{
  "name": "",
  "version": "0.0.1",
  "devDependencies": {},
  "dependencies": {},
  "scripts": {
    "lint": "eslint . --fix --ext .js,.json",
    "prettier": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "engines": {
    "node": ">10.0.0"
  }
}
